        <TextBlock x:Name="txtBlockFem" HorizontalAlignment="Left" Margin="95,246,0,0" TextWrapping="Wrap" Text="Fyll i antal femmor (1-5)" VerticalAlignment="Top"/>
        <TextBlock x:Name="txtBlockSex" HorizontalAlignment="Left" Margin="95,293,0,0" TextWrapping="Wrap" Text="Fyll i antal sexor (1-5)" VerticalAlignment="Top"/>
