        <TextBlock x:Name="txtBlockTotalSex" HorizontalAlignment="Left" Margin="223,309,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="btnTestSum" Content="Tvåor" HorizontalAlignment="Left" Margin="335,77,0,0" VerticalAlignment="Top" Height="33" Width="65" Click="btnTestSum_Click"/>
